        "just-ls": true,
        "latex": true,
        "lua": true,
        "make": true,
        "neocmake": true,
        "new-darcula": true,
        "nix": true,
        "nu": true,
        "plantuml": true,
        "powershell": true,
        "proto": true,
        "pylsp": true,
        "r": true,
        "rainbow-csv": true,
